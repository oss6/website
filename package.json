    "autoprefixer": "^10.2.4",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.3.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^10.0.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "moment": "^2.25.3"
  }
}
