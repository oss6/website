{
  "name": "website",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site && rimraf _tmp",
    "build": "npm run clean && NODE_ENV=production npx postcss tailwind.build.css --o _site/styles.css && ELEVENTY_PRODUCTION=true npx @11ty/eleventy",
